        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Reorder queue

//Reset the queue's first x value
queue_x = orig_x;
//If patron queue is not already empty
if(!ds_list_empty(patron_queue)) {
    //If patron to move exists
    if(patron_to_remove) {
        //Find his position in the list
        var pos = ds_list_find_index(patron_queue, patron_to_remove);
        //Remove him from the queue
        ds_list_delete(patron_queue, pos);
    }
    //Reorder the rest of the queue
    for (i = 0; i &lt; ds_list_size(patron_queue); i++)
    {
        //Update each patrons x and y
        patron_queue[| i].x = queue_x;
        //Move the x value to be used for the next object
        queue_x += 100;
    }
}
